  "dependencies": {
    "@google-cloud/storage": "^0.5.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.18.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-native": "^2.1.0",
